{
  "name": "infernode",
  "version": "0.0.0",
  "description": "flamegraph tool for node.js",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "clean": "rm -rf ./dist",
    "copy-assets": "cp -R ./src/assets ./dist/",
    "build": "tsc && NODE_ENV=production webpack && npm run copy-assets",
    "test": "tsc && npm run copy-assets && jest --coverage",
    "dev": "mkdir -p ./dist && touch ./dist/index.js && tsc && concurrently \"npx tsc --watch\" \"nodemon --ext js,json,html,css dist/index.js\" \"webpack serve --open\""
  },
  "jest": {
    "globalSetup": "./jest-setup.js",
    "globalTeardown": "./jest-teardown.js",
    "reporters": [
      "default",
      "github-actions"
    ]
  },
  "nodemonConfig": {
    "watch": [
      "./src/assets/**/*",
      "./dist/**/*.js"
    ],
    "ignore": [
      "./src/__tests__",
      "**/**.test.[jt]s[x]",
      "**/**.spec.[jt]s[x]",
      "./dist/bundle.js",
      "./dist/assets/**/*",
      "./dist/public/**/*"
    ],
    "events": {
      "restart": "npm run copy-assets"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oslabs-beta/infernode.git"
  },
  "keywords": [
    "flamegraph",
    "perf",
    "node.js"
  ],
  "author": "infernode dev team",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/oslabs-beta/infernode/issues"
  },
  "homepage": "https://github.com/oslabs-beta/infernode#readme",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.7",
    "@types/node": "^18.7.2",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/react-router": "^5.1.18",
    "@types/react-router-bootstrap": "^0.24.5",
    "@types/react-router-dom": "^5.3.3",
    "@types/sqlite3": "^3.1.8",
    "@types/supertest": "^2.0.12",
    "concurrently": "^7.3.0",
    "css-loader": "^6.7.1",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^28.1.3",
    "nodemon": "^2.0.19",
    "sass": "^1.54.4",
    "sass-loader": "^13.0.2",
    "style-loader": "^3.3.1",
    "supertest": "^6.2.4",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "typescript": "^4.7.4",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.10.0"
  },
  "dependencies": {
    "bootstrap": "^5.2.0",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "react": "^18.2.0",
    "react-bootstrap": "^2.5.0",
    "react-dom": "^18.2.0",
    "react-router-bootstrap": "^0.26.2",
    "react-router-dom": "^6.3.0",
    "sqlite3": "^5.0.11"
  }
}
