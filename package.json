{
  "name": "infernode",
  "version": "1.0.2-development",
  "description": "flamegraph tool for node.js",
  "main": "dist/index.js",
  "bin": {
    "infernode": "./dist/bin/infernode.js"
  },
  "sideEffects": false,
  "scripts": {
    "commit": "git-cz",
    "resetdb": "rimraf ./database/",
    "clean": "rimraf ./dist/*",
    "ts-lint-server": "eslint .",
    "ts-lint-client": "eslint ./src/public/infernode/",
    "lint": "npm-run-all css-lint ts-lint-server ts-lint-client",
    "copy-assets": "cp -R ./src/assets ./dist/",
    "prebuild": "npm-run-all lint clean copy-assets",
    "tsc": "tsc",
    "tsc-dev": "tsc --watch",
    "node-dev": "NODE_ENV=development nodemon --ext js,json,html,css dist/index.js | npx pino-pretty",
    "webpack": "webpack --config webpack.prod.js",
    "webpack-dev": "webpack serve --open --config webpack.dev.js",
    "build": "concurrently \"npm run css\" \"npm run tsc\" \"npm run webpack\"",
    "test": "npm-run-all build test-only",
    "test-only": "jest --coverage react-scripts test --env=jsdom",
    "start": "NODE_ENV=production node dist/index.js",
    "dev": "mkdir -p ./dist && touch ./dist/index.js && tsc && concurrently npm:css-dev npm:tsc-dev npm:node-dev npm:webpack-dev",
    "semantic-release": "semantic-release --branches main",
    "css-lint": "stylelint src/scss/",
    "css-compile": "sass --load-path=node_modules --style=compressed --source-map --embed-sources src/scss:dist/assets/css/",
    "css-dev": "npm run css-compile -- --watch",
    "css-prefix": "postcss --replace dist/assets/css/styles.css --use autoprefixer --map",
    "css": "npm-run-all css-compile css-prefix",
    "postbuild": "chmod +x dist/bin/infernode.js"
  },
  "jest": {
    "globalSetup": "./jest-setup.js",
    "globalTeardown": "./jest-teardown.js",
    "preset": "ts-jest",
    "testEnvironment": "node",
    "reporters": [
      "default",
      "github-actions"
    ],
    "transform": {
      "^.+\\.tsx$": "ts-jest",
      "^.+\\.ts$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect"
    ],
    "verbose": true
  },
  "nodemonConfig": {
    "watch": [
      "./src/assets/**/*",
      "./dist/controllers/**/*.js",
      "./dist/interfaces/**/*.js",
      "./dist/models/**/*.js",
      "./dist/routes/**/*.js",
      "./dist/utils/**/*.js",
      "./dist/*.js"
    ],
    "ignore": [
      "./src/__tests__",
      "**/**.test.[jt]s[x]",
      "**/**.spec.[jt]s[x]",
      "./dist/bundle.js",
      "./dist/assets/**/*",
      "./dist/public/**/*"
    ],
    "events": {
      "restart": "npm run copy-assets"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/oslabs-beta/infernode.git"
  },
  "keywords": [
    "flamegraph",
    "perf",
    "node.js"
  ],
  "author": "infernode dev team",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/oslabs-beta/infernode/issues"
  },
  "homepage": "https://github.com/oslabs-beta/infernode#readme",
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@svgr/webpack": "^6.3.1",
    "@testing-library/dom": "^8.17.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/deep-equal": "^1.0.1",
    "@types/express": "^4.17.13",
    "@types/formidable": "^2.0.5",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^28.1.7",
    "@types/node": "^18.7.2",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/react-redux": "^7.1.24",
    "@types/react-router": "^5.1.18",
    "@types/react-router-bootstrap": "^0.24.5",
    "@types/react-router-dom": "^5.3.3",
    "@types/sqlite3": "^3.1.8",
    "@types/supertest": "^2.0.12",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.33.1",
    "autoprefixer": "^10.4.8",
    "babel-preset-airbnb": "^5.0.0",
    "concurrently": "^7.3.0",
    "css-loader": "^6.7.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.22.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "file-loader": "^6.2.0",
    "fork-ts-checker-notifier-webpack-plugin": "^6.0.0",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "nodemon": "^2.0.19",
    "npm-run-all": "^4.1.5",
    "pino-pretty": "^9.1.0",
    "postcss": "^8.4.16",
    "postcss-cli": "^10.0.0",
    "rimraf": "^3.0.2",
    "sass": "^1.54.9",
    "sass-loader": "^13.0.2",
    "semantic-release": "^19.0.5",
    "style-loader": "^3.3.1",
    "stylelint": "^14.11.0",
    "stylelint-config-twbs-bootstrap": "^6.0.0",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.8",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.10.0",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.6",
    "@reduxjs/toolkit": "^1.8.5",
    "@types/pino": "^7.0.5",
    "@types/pino-http": "^5.8.1",
    "axios": "^0.27.2",
    "body-parser": "^1.20.0",
    "bootstrap": "^5.2.0",
    "deep-equal": "^2.0.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "formidable": "^2.0.1",
    "fs": "^0.0.1-security",
    "fs-extra": "^10.1.0",
    "open": "^8.4.0",
    "pino": "^8.4.2",
    "pino-http": "^8.2.0",
    "react": "^18.2.0",
    "react-bootstrap": "^2.5.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.2",
    "react-router-bootstrap": "^0.26.2",
    "react-router-dom": "^6.3.0",
    "sqlite3": "^5.0.11",
    "tslib": "^2.4.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
