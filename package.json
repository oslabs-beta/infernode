{
  "name": "infernode",
  "version": "1.0.0",
  "description": "flamegraph tool for node.js",
  "main": "dist/index.js",
  "sideEffects": false,
  "scripts": {
    "lint": " eslint . && eslint ./src/public/infernode/",
    "clean": "rimraf ./dist/*",
    "resetdb": "rimraf ./database/captures/*.perf ./database/captures/*.stacks ./database/folded/*.folded ./database/SVGs/*.svg ./database/uploads/* ./database/captureDB.db",
    "copy-assets": "cp -R ./src/assets ./dist/",
    "prebuild": "npm run lint && npm run clean && npm run copy-assets",
    "tsc": "tsc",
    "webpack": "webpack --config webpack.prod.js",
    "build": "npm run tsc && npm run webpack",
    "test": "npm run build && jest --coverage react-scripts test --env=jsdom",
    "test-only": "jest --coverage react-scripts test --env=jsdom",
    "start": "NODE_ENV=production node dist/index.js",
    "dev": "mkdir -p ./dist && touch ./dist/index.js && tsc && concurrently \"npx tsc --watch\" \"NODE_ENV=development nodemon --ext js,json,html,css dist/index.js\" \"webpack serve --open --config webpack.dev.js\""
  },
  "jest": {
    "globalSetup": "./jest-setup.js",
    "globalTeardown": "./jest-teardown.js",
    "preset": "ts-jest",
    "testEnvironment": "node",
    "reporters": [
      "default",
      "github-actions"
    ],
    "transform": {
      "^.+\\.tsx$": "ts-jest",
      "^.+\\.ts$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect"
    ],
    "verbose": true
  },
  "nodemonConfig": {
    "watch": [
      "./src/assets/**/*",
      "./dist/controllers/**/*.js",
      "./dist/interfaces/**/*.js",
      "./dist/models/**/*.js",
      "./dist/routes/**/*.js",
      "./dist/utils/**/*.js",
      "./dist/*.js"
    ],
    "ignore": [
      "./src/__tests__",
      "**/**.test.[jt]s[x]",
      "**/**.spec.[jt]s[x]",
      "./dist/bundle.js",
      "./dist/assets/**/*",
      "./dist/public/**/*"
    ],
    "events": {
      "restart": "npm run copy-assets"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oslabs-beta/infernode.git"
  },
  "keywords": [
    "flamegraph",
    "perf",
    "node.js"
  ],
  "author": "infernode dev team",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/oslabs-beta/infernode/issues"
  },
  "homepage": "https://github.com/oslabs-beta/infernode#readme",
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@svgr/webpack": "^6.3.1",
    "@testing-library/dom": "^8.17.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/axios": "^0.14.0",
    "@types/deep-equal": "^1.0.1",
    "@types/express": "^4.17.13",
    "@types/formidable": "^2.0.5",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^28.1.7",
    "@types/node": "^18.7.2",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/react-redux": "^7.1.24",
    "@types/react-router": "^5.1.18",
    "@types/react-router-bootstrap": "^0.24.5",
    "@types/react-router-dom": "^5.3.3",
    "@types/sqlite3": "^3.1.8",
    "@types/supertest": "^2.0.12",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "babel-preset-airbnb": "^5.0.0",
    "concurrently": "^7.3.0",
    "css-loader": "^6.7.1",
    "eslint": "^8.22.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "file-loader": "^6.2.0",
    "fork-ts-checker-notifier-webpack-plugin": "^6.0.0",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "nodemon": "^2.0.19",
    "rimraf": "^3.0.2",
    "sass": "^1.54.4",
    "sass-loader": "^13.0.2",
    "style-loader": "^3.3.1",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.8",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.10.0",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.5",
    "axios": "^0.27.2",
    "body-parser": "^1.20.0",
    "bootstrap": "^5.2.0",
    "deep-equal": "^2.0.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "formidable": "^2.0.1",
    "fs-extra": "^10.1.0",
    "react": "^18.2.0",
    "react-bootstrap": "^2.5.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.2",
    "react-router-bootstrap": "^0.26.2",
    "react-router-dom": "^6.3.0",
    "sqlite3": "^5.0.11"
  }
}
